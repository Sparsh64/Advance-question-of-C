#include<stdio.h>
#include<conio.h>
int main()
{
int num1,num2;
clrscr();
printf("Please enter two different numbers\n");
scanf("%d %d",&num1,&num2);
if(num1>num2)
{
printf("%d is largest\n",num1);
}
else if (num2>num1)
{
printf("%d is largest\n",num2);
}
else
{
printf("Both are equal\n");
}
getch();
return 0;
}
